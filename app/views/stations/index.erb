<h1>Stations</h1>
<a class="btn btn-primary" href="/stations/new">
    Create a New Station</a><br>

<a class="btn btn-primary" href="/stations-dashboard">
    Dashboard</a><br>

<div>
  <ul>
  <% @stations.each do |station| %>
      <h2><a href="/stations/<%= station.id %>"><%= station.name %>, <%= station.city.name %></a></h2>
  <% end %>
</ul>
</div>
