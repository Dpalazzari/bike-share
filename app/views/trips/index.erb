<div class="container">
  <h2>Trips</h2>
  <div class="list-group">
    <% @trips.each do |trip| %>
      <a href="/trips/<%= trip.id %>" class="list-group-item">
        <%= trip.start_information %><br>
      <%= trip.end_information %></br></a>
    <% end %>
  </div>
</div>

<ul class="pagination pagination-lg">
  <% (1..@pages).each do |num| %>
      <li><a href="/trips/page/<%= num %>"><%= num %></a></li>
    <% end %>
</ul>
